<?xml version='1.0' encoding='utf-8'?>
<widget id="com.reece.hackathon" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hackathon</name>
    <description>
        RFID Hack
    </description>
    <author email="matt.ng@reece.com.au" href="http://reece.com.au">
        Reece Hackathon
    </author>
    <content src="index.html" />
    <plugin name="phonegap-nfc" source="npm" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
